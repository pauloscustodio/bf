spew("$test.in", <<END);
		alloc_cell8(A)
		alloc_cell8(B)
		mod8(A, B)
END
capture_ok("bfpp $test.in | bf -D", <<END);
Tape:  0 
     ^^^ (ptr=0)

END

# run mod8(a,b)
for my $A (0, 10, 20) {
	for my $B (1, 2, 3, 4) {
		spew("$test.in", <<END);
		alloc_cell8(A)
		alloc_cell8(B)
		set8(A, $A)
		set8(B, $B)
		mod8(A, B)
		>B
END
		my $R = sprintf("%3d", $A % $B);
		capture_ok("bfpp $test.in | bf -D", <<END);
Tape:$R   $B 
         ^^^ (ptr=1)

END
	}
}

