$test.in:1:8: error: expected ')' at end of macro call, found ','
END

# div(a,b)
spew("$test.in", <<END);
alloc_cell(A)
alloc_cell(B)
div(A,B)
>A
END
capture_ok("bfpp $test.in", <<END);
