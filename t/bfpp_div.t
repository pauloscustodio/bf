#!/usr/bin/env perl

BEGIN { use lib 't'; require 'testlib.pl'; }

use Modern::Perl;

# div - error no arguments
spew("$test.in", "div");
capture_nok("bfpp $test.in", <<END);
$test.in:1:4: error: expected '(' after macro name 'div'
END

# div - error empty arguments
spew("$test.in", "div()");
