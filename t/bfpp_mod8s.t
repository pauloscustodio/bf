sub mod8s {
    my ($a, $b) = @_;

    # convert to signed 8-bit
    $a = ($a & 0xFF); $a -= 256 if $a >= 128;
    $b = ($b & 0xFF); $b -= 256 if $b >= 128;

    # signed remainder: same sign as a
    my $r = $a - int($a/$b) * $b;

    return $r & 0xFF;
}

