# run div8s(a,b)
for my $A (-20, -10, 0, 10, 20) {
	for my $B (-4, -3, -2, -1, 1, 2, 3, 4) {
		my $A8bit = $A & 0xFF;
		my $B8bit = $B & 0xFF;
		spew("$test.in", <<END);
		alloc_cell8(A)
		alloc_cell8(B)
		set8(A, $A8bit)
		set8(B, $B8bit)
		div8s(A, B)
		>B
END
		my $R = sprintf("%3d", int($A / $B) & 0xFF);
		my $Bout = sprintf("%3d", $B & 0xFF);
		capture_ok("bfpp $test.in | bf -D", <<END);
Tape:$R $Bout 
         ^^^ (ptr=1)

END
	}
