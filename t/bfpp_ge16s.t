#!/usr/bin/env perl

BEGIN { use lib 't'; require 'testlib.pl'; }

use Modern::Perl;

# ge16s - error no arguments
spew("$test.in", "ge16s");
capture_nok("bfpp $test.in", <<END);
$test.in:1:6: error: expected '(' after macro name 'ge16s'
END

# ge16s - error empty arguments
spew("$test.in", "ge16s()");
capture_nok("bfpp $test.in", <<END);
$test.in:1:8: error: macro 'ge16s' expects 2 arguments
END

# ge16s - error too many arguments
spew("$test.in", "ge16s(A,B,C)");
capture_nok("bfpp $test.in", <<END);
$test.in:1:10: error: expected ')' at end of macro call, found ','
END

# ge16s(a,b)
spew("$test.in", <<END);
alloc_cell16(A)
alloc_cell16(B)
ge16s(A,B)
END
capture_ok("bfpp $test.in", <<END);
